<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
	
	
	<application>
		<resource-bundle>
			<base-name>messages.app</base-name>
			<var>app</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>messages.taskStatus</base-name>
			<var>ts</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>messages.dataTable</base-name>
			<var>dataTable</var>
		</resource-bundle>
	</application>
</faces-config>